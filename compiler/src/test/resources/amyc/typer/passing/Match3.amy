object Constructors
  abstract class Foo 
  case class Bar(i: Int) : Foo
  case class Baz(b: Boolean, f: Foo) : Foo

  abstract class Foo2
  case class Bar2(i: Int) : Foo2
  case class Baz2(b: Boolean, f: Foo2) : Foo2

  val x: Boolean = Bar(1) match {
    case Bar(1) => true
    case Baz(x, Baz(y, Bar(z))) =>
      x || y && z < 0
    case Baz(_, Bar(_)) => false
  };
  x
end Constructors
